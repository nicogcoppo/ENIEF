FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5000; // Es el número de iteraciones

deltaT          1; // es el paso 

writeControl    timeStep;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression compressed; // escribe más rápido

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


functions
{
    #include "forces"
    // #include "forceCoeffs"
    // #includeFunc "probes"
    // #includeFunc "singleGraph"
}



